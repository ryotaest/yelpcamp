<% if(success&&success.length){ %>

<div
  class="alert alert-success aleart-dismissible fade show d-flex justify-content-between"
  role="alert"
>
  <%= success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> <% if(error&&error.length){ %>

<div
  class="alert alert-danger aleart-dismissible fade show d-flex justify-content-between"
  role="alert"
>
  <%= error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>
